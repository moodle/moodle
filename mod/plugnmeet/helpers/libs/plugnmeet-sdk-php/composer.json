{
  "name": "mynaparrot/plugnmeet-sdk",
  "description": "plugNmeet PHP SDK",
  "type": "library",
  "license": "MIT",
  "version": "1.1.1",
  "autoload": {
    "psr-4": {
      "Mynaparrot\\Plugnmeet\\": "src/"
    }
  },
  "authors": [
    {
      "name": "Jibon Costa",
      "email": "jibon@mynaparrot.com"
    }
  ],
  "scripts": {
    "phpcs": "phpcs --standard=PSR12 src",
    "phpcbf": "phpcbf --standard=PSR12 src"
