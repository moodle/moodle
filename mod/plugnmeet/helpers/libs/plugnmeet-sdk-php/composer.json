  "require": {
    "php": "^7.4|^8.0",
    "ext-curl": "*",
    "ext-json": "*",
    "ramsey/uuid": "4.2",
    "firebase/php-jwt": "^6.0"
  },
  "require-dev": {
    "overtrue/phplint": "^3.0",
    "squizlabs/php_codesniffer": "3.*"
  }
