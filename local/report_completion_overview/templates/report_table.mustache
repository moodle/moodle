{{!
    @template local_report_completion_overview/report_table

    Report table template.

    Context variables required for this template:
    * bulkmoveoptions - array - Key-value pair array for the options of the the bulk move select menu element.

    Example context (json):
    {
    }
}}
<div id="completion_overview_table">
    <table id="report_completion_overview">
        <thead>
            <tr>
                {{#tableheaders}}
                    <th>{{name}}</th>
                {{/tableheaders}}
            </tr>
        </thead>
        <tbody>
            {{#tabledata}}
                <tr>
                    {{#rowdata}}
                        <td>{{value}}</td>
                    {{/rowdata}}
                </tr>
            {{/tabledata}}
        <tbody>
    </table>
</div>

