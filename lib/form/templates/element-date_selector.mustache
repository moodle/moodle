{{< core_form/element-group }}
    {{$element}}
        <fieldset data-fieldtype="date" class="m-0 p-0 border-0" id="{{element.id}}">
            <legend class="visually-hidden">{{label}}</legend>
            <span class="fdate_selector d-flex flex-wrap align-items-center">
            {{#element.elements}}
                {{{separator}}}
                {{{html}}}
            {{/element.elements}}
            </span>
        </fieldset>
    {{/element}}
{{/ core_form/element-group }}
